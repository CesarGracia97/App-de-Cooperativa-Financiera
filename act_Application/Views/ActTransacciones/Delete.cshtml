@model act_Application.Models.BD.ActTransaccione

@{
    ViewData["Title"] = "Eliminar Transacciones";
}
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/css/Administracion/estiloEliminar.css" />
</head>
<div class="content-text">
    <h1>Eliminar Transacciones</h1>
    <hr />
    <div class="container">
        <table>
            <tr>
                <td class="titulo">Razon</td>
                <td class="texto">@Html.DisplayFor(model => model.Razon)</td>
            </tr>
            <tr>
                <td class="titulo">Id del Usuario</td>
                <td class="texto">@Html.DisplayFor(model => model.IdUser)</td>
            </tr>
        </table>
    </div>
    <div class="btn-container">
        <form asp-action="Delete">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Eliminar" class="btn-delite" /> |
            <a class="btn-return" asp-action="Index">Regresar</a>
        </form>
    </div>
</div>
