@model act_Application.Models.BD.ActNotificacione

@{
    ViewData["Title"] = "Delete";
}
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/css/Administracion/estiloEliminar.css" />
</head>

<div class="content-text">
    <h1>Eliminar Notificacion</h1>
    <hr />
    <div class="container">
        <table>
            <tr>
                <td class="titulo">Id User</td>
                <td class="texto">@Html.DisplayFor(model => model.IdUser)</td>
            </tr>
            <tr>
                <td class="titulo">Razon</td>
                <td class="texto">@Html.DisplayFor(model => model.Razon)</td>
            </tr>
            <tr>
                <td class="titulo">Descripcion</td>
                <td class="texto">@Html.DisplayFor(model => model.Descripcion)</td>
            </tr>
            <tr>
                <td class="titulo">Fecha Notificacion</td>
                <td class="texto">@Html.DisplayFor(model => model.FechaNotificacion)</td>
            </tr>
        </table>
    </div>
    <div class="btn-container">
        <form asp-action="Delete">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Eliminar" class="btn-delite" />
            <a class="btn-return" asp-action="Index">Regresar</a>
        </form>
    </div>
</div>

